{
  "type": "Screen",
  "attributes": {
    "id": "page.settings.store.shipping",
    "safe": false
  },
  "children": [
    {
      "type": "Column",
      "attributes": {},
      "children": [
        {
          "type": "Divider",
          "attributes": {
            "height": 2.0,
            "color": "#8c8c8c"
          },
          "children": []
        },
        {
          "type": "Flexible",
          "attributes": {
            "flex": 1,
            "fit": "Tight"
          },
          "children": [
            {
              "type": "ListView",
              "attributes": {
                "separator": true,
                "separatorColor": "#8c8c8c"
              },
              "children": [
                {
                  "type": "ListItemSwitch",
                  "attributes": {
                    "caption": "Local Pickup",
                    "subCaption": "Pick up orders in your store",
                    "name": "value",
                    "selected": false
                  },
                  "children": [],
                  "action": {
                    "type": "Command",
                    "url": "http://localhost:0/commands/enable-shipping?type=LOCAL_PICKUP"
                  }
                },
                {
                  "type": "ListItemSwitch",
                  "attributes": {
                    "caption": "Local Delivery",
                    "subCaption": "Deliver orders to local customers",
                    "name": "value",
                    "selected": false
                  },
                  "children": [],
                  "action": {
                    "type": "Command",
                    "url": "http://localhost:0/commands/enable-shipping?type=LOCAL_DELIVERY"
                  }
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "appBar": {
    "type": "AppBar",
    "attributes": {
      "title": "Shipping",
      "elevation": 0.0,
      "backgroundColor": "#FFFFFF",
      "foregroundColor": "#000000"
